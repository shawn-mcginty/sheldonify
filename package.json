{
  "name": "sheldonify",
  "version": "0.1.0",
  "description": "Replaces every word in a string with the largest found synonym making your message impossible to read!",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "npm run transpile",
    "prestart": "npm run transpile",
    "start": "node --harmony --harmony_arrow_functions boot_server.js",
    "pretranspile": "astro jshint",
    "transpile": "astro babel",
    "pretest": "npm run transpile",
    "test": "astro test --watch",
    "nowatchtest": "astro test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shawn-mcginty/sheldonify.git"
  },
  "keywords": [
    "sheldon",
    "sheldonify",
    "awesome"
  ],
  "author": "Shawn McGinty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shawn-mcginty/sheldonify/issues"
  },
  "homepage": "https://github.com/shawn-mcginty/sheldonify#readme",
  "dependencies": {
    "chai": "^3.3.0",
    "mocha": "^2.3.3",
    "rewire": "^2.3.4",
    "sinon": "^1.17.0"
    "astro": "^0.7.1",
    "async": "^1.4.2"
  }
}
